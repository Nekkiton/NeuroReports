{
  "name": "reportkeeper",
  "version": "1.4.1",
  "description": "Report Keeper crossplatform app.",
  "repository": "https://github.com/iamusualguy/NeuroReports.git",
  "main": "main.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"NeuroReports\"",
    "package-win64": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"NeuroReports\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Anton Stepanov, Andrey Kovanov, Ovsov Nikita, Michail Lipin",
  "license": "ISC",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-json-storage": "^4.1.5",
    "electron-spellchecker": "^1.1.2",
    "exceljs": "^1.6.3",
    "jquery": "^3.3.1",
    "update-electron-app": "^1.3.0",
    "xls-to-json": "^0.5.1"
  },
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-packager": "^13.0.0",
    "electron-rebuild": "^1.8.2"
  }
}
